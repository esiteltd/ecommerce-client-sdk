{
	"name": "@esiteltd/ecommerce-client-sdk",
	"version": "0.0.4",
	"description": "TypeScript SDK for Ecommerce API",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/esiteltd/ecommerce-client-sdk.git"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/@esiteltd"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./client": {
			"types": "./dist/client.d.ts",
			"import": "./dist/client.mjs",
			"require": "./dist/client.js",
			"default": "./dist/client.js"
		}
	},
	"files": [
		"dist/**/*"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage",
		"prepublishOnly": "npm run build",
		"typecheck": "tsc --noEmit"
	},
	"author": "Sajad",
	"license": "MIT",
	"peerDependencies": {
		"zod": "^3.22.4"
	},
	"dependencies": {
		"react": ">=18.0.0",
		"react-dom": ">=18.0.0",
		"uuid": "^11.1.0",
		"zustand": "^5.0.7"
	},
	"devDependencies": {
		"@types/node": "^20.10.0",
		"@types/react": "^19",
		"@vitest/ui": "^1.0.0",
		"tsup": "^8.0.0",
		"typescript": "^5.3.0",
		"vitest": "^1.0.0"
	},
	"engines": {
		"node": ">=16.0.0"
	}
}
