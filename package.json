{
	"name": "ecommerce-sdk",
	"version": "1.0.0",
	"description": "TypeScript SDK for Ecommerce API",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist/**/*"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage",
		"prepublishOnly": "npm run build",
		"typecheck": "tsc --noEmit"
	},
	"author": "Sajad",
	"license": "MIT",
	"peerDependencies": {
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@types/node": "^20.10.0",
		"@vitest/ui": "^1.0.0",
		"tsup": "^8.0.0",
		"typescript": "^5.3.0",
		"vitest": "^1.0.0"
	},
	"engines": {
		"node": ">=16.0.0"
	}
}
